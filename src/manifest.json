{
  "manifest_version": 2,
  "name": "GPOscar",
  "description": "Chrome Extension which overlays average GPAs of teachers onto Georgia Tech's Oscar",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "https://critique.gatech.edu/course.php?id=*"
  ],
  "content_scripts": [
    {
      "matches": ["https://oscar.gatech.edu/pls/bprod/bwskfcls.P_GetCrse"],
      "js": ["showGPAs.js"]
    }
  ]
}